<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Backup SharedPreferences -->
    <include domain="sharedpref" path="YouniFirstPrefs.xml"/>
    <include domain="sharedpref" path="UserDataBackup.xml"/>

    <!-- Backup files in internal storage -->
    <include domain="file" path="user_session_data.xml"/>
    <include domain="file" path="cookies_backup.json"/>

    <!-- Exclude cache and temp files -->
    <exclude domain="database" path="cache.db"/>
    <exclude domain="file" path="cache/"/>
    <exclude domain="file" path="temp/"/>
</full-backup-content>